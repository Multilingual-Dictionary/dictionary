<p id="notice"><%= notice %></p>

<h1>Dict Configs</h1>

<table class="table table-hover table-bordered ">
  <thead>
    <tr bgcolor=tabhead_color>
      <th>Dict sys name</th>
      <th>Dict name</th>
      <th>Lang</th>
      <th>Xlate lang</th>
      <th>Desc</th>
      <th>Protocol</th>
      <th>Url</th>
      <th>Syntax</th>
      <th>Ext infos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dict_configs.each do |dict_config| %>
      <tr>
        <td><%= dict_config.dict_sys_name %></td>
        <td><%= dict_config.dict_name %></td>
        <td><%= dict_config.lang %></td>
        <td><%= dict_config.xlate_lang %></td>
        <td><%= dict_config.desc %></td>
        <td><%= dict_config.protocol %></td>
        <td><%= dict_config.url %></td>
        <td><%= dict_config.syntax %></td>
        <td><%= dict_config.ext_infos %></td>
<div class="btn-group">
        <td><%= link_to 'Show', dict_config %></td>
        <td><%= link_to 'Edit', edit_dict_config_path(dict_config) %></td>
        <td><%= link_to 'Destroy', dict_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</div>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dict Config', new_dict_config_path %>
